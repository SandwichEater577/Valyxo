{
  "name": "valyxo-native",
  "version": "0.5.2",
  "description": "Native Rust backend for Valyxo desktop app",
  "main": "index.js",
  "types": "index.d.ts",
  "author": "NesTea",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SandwichEater577/Valyxo"
  },
  "napi": {
    "name": "valyxo-native",
    "triples": {
      "defaults": true,
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-gnu",
        "x86_64-unknown-linux-musl"
      ]
    }
  },
  "scripts": {
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "node test.js"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.0"
  },
  "engines": {
    "node": ">= 18"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "*.node"
  ]
}
